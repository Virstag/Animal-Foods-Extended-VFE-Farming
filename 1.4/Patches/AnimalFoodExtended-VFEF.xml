<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Animal Foods Extended</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/ThingDef[defName = "PetFood"]</xpath>
					<value><researchPrerequisites><li>VFE_Res_FarmingTechniques</li></researchPrerequisites></value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/ThingDef[defName = "GrazingFeedstock"]</xpath>
					<value><researchPrerequisites><li>VFE_Res_FarmingTechniques</li></researchPrerequisites></value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/ThingDef[defName = "RenderedMeat"]</xpath>
					<value><researchPrerequisites><li>VFE_Res_FarmingTechniques</li></researchPrerequisites></value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>